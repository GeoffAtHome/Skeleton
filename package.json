{
  "devDependencies": {
    "@open-wc/building-rollup": "^1.0.0",
    "@open-wc/eslint-config": "^4.2.0",
    "@open-wc/testing": "^2.0.0",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^2.20.0",
    "@web/dev-server": "^0.1.1",
    "@web/dev-server-storybook": "^0.3.4",
    "@web/test-runner": "^0.11.5",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^2.6.3",
    "rollup": "^2.38.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "browser": {
    "crypto": false
  },
  "scripts": {
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "test": "tsc && wtr --coverage",
    "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\"",
    "storybook": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds -c .storybook/server.mjs\"",
    "storybook:build": "tsc && build-storybook",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "start:build": "npm run build && web-dev-server --root-dir dist --app-index index.html --open --compatibility none",
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\" --open auto"
  },
  "name": "my-app",
  "version": "0.0.0",
  "description": "Webcomponent my-app following open-wc recommendations",
  "author": "my-app",
  "license": "MIT",
  "dependencies": {
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-checkbox": "^0.20.0",
    "@material/mwc-drawer": "^0.20.0",
    "@material/mwc-tab": "^0.20.0",
    "@material/mwc-tab-bar": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@material/mwc-top-app-bar": "^0.20.0",
    "@polymer/polymer": "^3.4.1",
    "@pwabuilder/pwainstall": "^1.6.7",
    "@pwabuilder/pwaupdate": "^0.2.0",
    "@supabase/supabase-js": "^1.6.0",
    "@types/events": "^3.0.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "events": "^3.2.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "plastic-image": "^3.0.2",
    "pouchdb": "^7.2.2",
    "pouchdb-authentication": "^1.1.3",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "import/no-unresolved": "off",
      "import/extensions": [
        "warn",
        "never",
        {
          "ignorePackages": true
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
